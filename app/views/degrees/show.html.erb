<h1>Détails du diplôme</h1>

<dl>
  <dt>Institution:</dt>
  <dd><%= @degree.institution %></dd>

  <dt>Diplôme:</dt>
  <dd><%= @degree.degree %></dd>

  <dt>Date de début:</dt>
  <dd><%= @degree.start_date %></dd>

  <dt>Date de fin:</dt>
  <dd><%= @degree.end_date %></dd>
</dl>

<%= link_to "Modifier", edit_degree_path(@degree) %> |
<%= link_to "Supprimer", @degree, method: :delete, data: { confirm: "Êtes-vous sûr de vouloir supprimer ce diplôme ?" } %> |
<%= link_to "Retour à la liste des diplômes", degrees_path %>
